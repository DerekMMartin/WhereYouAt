<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="WhereYouAt.views.Story"
             xmlns:local="clr-namespace:WhereYouAt">
  
    <ContentView.Content>
        <ScrollView Orientation="Horizontal" Padding="0" Margin="10">
            <StackLayout x:Name="Lay" Orientation="Horizontal">
                <Frame x:Name="ProfileFrame" BorderColor="Black" Margin="5, 5, 20, 5">
                    <Image x:Name="ProfileImage" />
                </Frame>
            </StackLayout>
        </ScrollView>
  </ContentView.Content>
</ContentView>